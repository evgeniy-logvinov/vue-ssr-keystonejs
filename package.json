    "connect-mongo": "^1.3.2",
    "cross-env": "^5.0.4",
    "express": "^4.15.3",
    "keystone": "^4.0.0-beta.5",
    "lru-cache": "^4.1.1",
    "mongodb": "^2.2.30",
    "universal-cookie": "^2.0.8",
    "v-tooltip": "^2.0.0-rc.2",
    "vue": "^2.4.2",
    "vue-directive-tooltip": "^1.4.0",
    "vue-router": "^2.7.0",
    "vue-server-renderer": "^2.4.2",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
